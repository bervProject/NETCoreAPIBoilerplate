    <PackageReference Include="AWSSDK.S3" Version="3.7.8.27" />
    <PackageReference Include="AWSSDK.SimpleEmail" Version="3.7.0.154" />
    <PackageReference Include="Azure.Storage.Blobs" Version="12.12.0" />
